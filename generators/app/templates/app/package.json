{
	"name": "",
	"version": "1.0.0",
	"scripts": {
    "postinstall": "typings install --save dt~mocha dt~chai dt~sinon --global",
		"clean": "rimraf dist",
		"prebuild": "npm run clean",
		"build": "npm-run-all build:*",
		"build:index": "pug index.pug --out dist",
		"build:js": "webpack --watch",
		"watch:index": "pug index.pug --watch --out dist",
		"serve": "browser-sync start --server dist --files='dist/**/*.*'",
		"start": "npm-run-all --parallel build 'watch:index' serve"
	},
	"devDependencies": {
		"autoprefixer": "^6.5.1",
		"autoprefixer-loader": "^3.2.0",
		"browser-sync": "^2.7.10",
		"chai": "^3.5.0",
		"cpx": "^1.5.0",
		"css-loader": "^0.25.0",
		"extract-text-webpack-plugin": "^1.0.1",
		"karma": "^1.3.0",
		"karma-chai": "^0.1.0",
		"karma-cli": "^1.0.1",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.1",
		"karma-phantomjs-launcher": "^1.0.2",
		"karma-sinon": "^1.0.5",
		"karma-webpack": "^1.8.0",
		"mocha": "^3.2.0",
		"node-sass": "^3.4.2",
		"npm-run-all": "^3.1.1",
		"phantomjs-prebuilt": "^2.1.13",
		"pug": "^2.0.0-beta6",
		"rimraf": "^2.5.4",
		"sass-loader": "^4.0.2",
		"sinon": "^1.17.6",
		"style-loader": "^0.13.1",
		"ts-loader": "^0.9.5",
		"typescript": "^2.0.6",
		"typings": "^2.0.0",
		"webpack": "^1.13.3",
		"yarn": "^0.16.1"
	},
	"dependencies": {}
}
